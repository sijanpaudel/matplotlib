   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x=[1,2,3,4,5]\n",
    "y=[3,2,3,5,4]\n",
    "\n",
    "plt.step(x,y,color='r',marker='o',ms=10,mfc='k',label='python')\n",
    "plt.title(\"Python\")\n",
    "plt.xlabel('X-axis')\n",
    "plt.ylabel('Y-axis')\n",
    "plt.grid()\n",
    "plt.legend(loc=1)\n",
    "\n",
    "plt.show()"
   ]
